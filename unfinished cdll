#include<stdio.h>
#include<stdlib.h>
#include<string.h>
typedef struct linkedlist{
	char *data;
	struct linkedlist *prev;
	struct linkedlist *next;
}ll;
int main(int argc,char *argv[])
{
int flag=1,i=0,k;
char input[100];
while(flag)
{
ll *head;
printf("enter 1/2/3/4 for insert delete display exit\n");
scanf("%d",&k);
switch(k)
	{
		case 1: insert(ll *head);break;
		case 2: delete(ll *head);break;
		case 3: display(ll *head);break;
		case 4: flag=0;break;
	}
printf("successfully executed\n);
}		
return 0;
}
